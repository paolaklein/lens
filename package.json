{
  "name": "lens-web-componets",
  "description": "A web component library for clinical data search and visualisation",
  "version": "0.0.0",
  "type": "module",
  "module": "dist/lib/lens-web-components.js",
  "main": "dist/lib/lens-web-components.umd.js",
  "scripts": {
    "start": "npm run dev -s",
    "dev": "vite --config vite.demo.config.ts",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview --config vite.demo.config.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint-staged": "lint-staged",
    "prepublishOnly": "npm run build -s"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "husky": "^9.0.9",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.4.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  },
  "files": [
    "dist/lib"
  ],
  "dependencies": {
    "chart.js": "^4.4.0",
    "svelte-dnd-action": "^0.9.26",
    "uuid": "^9.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,svelte}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
